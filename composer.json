{
    "name": "sureshinde/orchestra-database",
    "description": "Database Component for Orchestra Platform",
    "keywords": ["orchestra-platform", "orchestral", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Suresh Shinde",
            "email": "suresh@sureshinde.com",
            "homepage": "https://github.com/sureshinde"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Database\\": ""
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "laravie/query-filter": "dev-master"
    },
    "suggest": {
        "laravie/query-filter": "Allow to use whereLike query builder macros with Laravel (dev-master)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        },
        "laravel": {
            "providers": [
                "SureShinde\\OrchestraDatabase\\CachableQueryServiceProvider",
                "SureShinde\\OrchestraDatabase\\SearchServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
